		"@types/node": "14.14.x",
		"@typescript-eslint/eslint-plugin": "4.12.x",
		"@typescript-eslint/parser": "4.12.x",
		"eslint": "7.17.x",
		"eslint-import-resolver-typescript": "2.3.x",
		"eslint-plugin-import": "2.22.x",
		"rollup": "2.38.x",
		"rollup-plugin-dts": "2.0.x",
